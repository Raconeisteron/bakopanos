<!-- 
This resource dictionary that provides the datatempletes for all viewmodels
-->
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:vm="clr-namespace:DemoApp.ViewModel" xmlns:vw="clr-namespace:DemoApp.View">

	<DataTemplate DataType="{x:Type vm:AnnouncementsViewModel}">
		<vw:AnnouncementsView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type vm:ContactsViewModel}">
		<vw:ContactsView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type vm:LinksViewModel}">
		<vw:LinksView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type vm:EditAnnouncementViewModel}">
		<vw:EditAnnouncementsView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type vm:EventsViewModel}">
		<vw:EventsView />
	</DataTemplate>

</ResourceDictionary>