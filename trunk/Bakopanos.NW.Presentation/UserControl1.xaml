<UserControl x:Class="Bakopanos.NW.Presentation.UserControl1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="300" Width="300" Loaded="UserControl_Loaded">
    <Grid>
        <TextBox x:Name="txtProduct_ID" IsReadOnly="True" 
                 Text="{Binding Path='Product ID', Mode=OneWay}" 
        />        
            
    </Grid>
</UserControl>
