<?xml version="1.0" encoding="utf-8" ?>

<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <UsingTask TaskName="DumpVisualStudioProjectList"
    AssemblyFile="ArchiCop.dll" />
  <UsingTask TaskName="CheckProjectProperties"
    AssemblyFile="ArchiCop.dll" />
  
  <Target Name="Dump">
    <DumpVisualStudioProjectList
      RootPath="C:\Chinook2\source"
      DumpFile="C:\Chinook2\source\archicop.xml"
      />
    </Target>
  
    <Target Name="Check">
    <CheckProjectProperties
      DumpFile="C:\Chinook2\source\archicop.xml"
      ConnectionString="Provider=Microsoft.Jet.OLEDB.4.0;Data Source=template.xls;Extended Properties=Excel 8.0;HDR=YES;"
      ProviderName="System.Data.OleDb"
      Table="Check"
      />    
  </Target>
</Project>